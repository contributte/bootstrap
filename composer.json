{
  "name": "contributte/bootstrap",
  "description": "Extra contrib to nette/boostrap",
  "keywords": [
    "nette",
    "application",
    "bootstrap",
    "docker"
  ],
  "type": "library",
  "license": "MIT",
  "homepage": "https://github.com/contributte/bootstrap",
  "authors": [
    {
      "name": "Milan Felix Å ulc",
      "homepage": "https://f3l1x.io"
    }
  ],
  "require": {
    "php": "^7.2",
    "nette/bootstrap": "~2.4.6",
    "nette/utils": "~2.5.3"
  },
  "require-dev": {
    "ninjify/qa": "^0.8.0",
    "ninjify/nunjuck": "^0.2.0",
    "phpstan/extension-installer": "^1.0",
    "phpstan/phpstan-deprecation-rules": "^0.11",
    "phpstan/phpstan-nette": "^0.11",
    "phpstan/phpstan-shim": "^0.11",
    "phpstan/phpstan-strict-rules": "^0.11"
  },
  "autoload": {
    "psr-4": {
      "Contributte\\Bootstrap\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\Mocks\\": "tests/mocks"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.4-dev"
    }
  }
}
